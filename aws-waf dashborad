<form>
  <label>Aws-Waf</label>
  <search id="base_search">
    <query>index="aws-waf" | table _time,action,httpRequest.country,httpRequest.clientIp,httpRequest.httpVersion,url,uri_query,httpRequest.httpMethod,http_user_agent
    | eval date=strftime(_time, "%d-%m-%Y")
    | eval time=strftime(_time, "%H:%M:%S.%Q")</query>
    <earliest>$time.earliest$</earliest>
    <latest>$time.latest$</latest>
  </search>
  <fieldset submitButton="true" autoRun="true">
    <input type="dropdown" token="country" searchWhenChanged="true">
      <label>Country</label>
      <choice value="*">ALL</choice>
      <default>*</default>
      <initialValue>*</initialValue>
      <fieldForLabel>httpRequest.country</fieldForLabel>
      <fieldForValue>httpRequest.country</fieldForValue>
      <search base="base_search">
        <query>| search httpRequest.clientIp="$client_ip$" httpRequest.clientIp="$blocked_ip$" | stats count by httpRequest.country</query>
        <earliest>0</earliest>
        <latest></latest>
      </search>
    </input>
    <input type="dropdown" token="client_ip" searchWhenChanged="true">
      <label>Client IP's</label>
      <choice value="*">ALL</choice>
      <default>*</default>
      <initialValue>*</initialValue>
      <fieldForLabel>httpRequest.clientIp</fieldForLabel>
      <fieldForValue>httpRequest.clientIp</fieldForValue>
      <search base="base_search">
        <query>| search httpRequest.country="$country$" httpRequest.clientIp="$blocked_ip$" | dedup httpRequest.clientIp | stats count by httpRequest.clientIp</query>
        <earliest>0</earliest>
        <latest></latest>
      </search>
    </input>
    <input type="dropdown" token="blocked_ip" searchWhenChanged="true">
      <label>Blocked IP's</label>
      <choice value="*">ALL</choice>
      <default>*</default>
      <initialValue>*</initialValue>
      <fieldForLabel>httpRequest.clientIp</fieldForLabel>
      <fieldForValue>httpRequest.clientIp</fieldForValue>
      <search base="base_search">
        <query>| search httpRequest.country="$country$" httpRequest.clientIp="$client_ip$" | spath action | dedup httpRequest.clientIp | search action=BLOCK</query>
        <earliest>0</earliest>
        <latest></latest>
      </search>
    </input>
    <input type="time" token="time" searchWhenChanged="true">
      <label>Quick Search</label>
      <default>
        <earliest>rt-30m</earliest>
        <latest>rt</latest>
      </default>
    </input>
  </fieldset>
  <row>
    <panel>
      <title>AWS-WAF Overview</title>
      <single>
        <title>Total Requests</title>
        <search base="base_search">
          <query>| search httpRequest.clientIp="$client_ip$" httpRequest.clientIp="$blocked_ip$" httpRequest.country="$country$" | stats count</query>
          <earliest>$time.earliest$</earliest>
          <latest>$time.latest$</latest>
        </search>
        <option name="colorMode">block</option>
        <option name="drilldown">none</option>
        <option name="numberPrecision">0</option>
        <option name="rangeColors">["0x53a051","0x006d9c"]</option>
        <option name="rangeValues">[0]</option>
        <option name="underLabel">Total Requests</option>
        <option name="unitPosition">after</option>
        <option name="useColors">1</option>
      </single>
    </panel>
  </row>
  <row>
    <panel>
      <map>
        <title>Map View</title>
        <search base="base_search">
          <query>| search httpRequest.clientIp="$client_ip$" httpRequest.clientIp="$blocked_ip$" httpRequest.country="$country$"
          | iplocation httpRequest.clientIp
          | geostats latfield=lat longfield=lon count</query>
          <earliest>$time.earliest$</earliest>
          <latest>$time.latest$</latest>
        </search>
        <option name="drilldown">none</option>
        <option name="mapping.map.center">(31.05,48.52)</option>
        <option name="mapping.map.zoom">2</option>
        <option name="mapping.markerLayer.markerMaxSize">40</option>
        <option name="mapping.markerLayer.markerOpacity">0.9</option>
        <option name="mapping.showTiles">1</option>
        <option name="mapping.tileLayer.maxZoom">19</option>
        <option name="mapping.tileLayer.url">http://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png</option>
        <option name="mapping.type">marker</option>
      </map>
    </panel>
  </row>
  <row>
    <panel>
      <chart>
        <title>Request Type</title>
        <search base="base_search">
          <query>| search httpRequest.clientIp="$client_ip$" httpRequest.clientIp="$blocked_ip$" httpRequest.country="$country$"
          | stats count by httpRequest.httpMethod</query>
          <earliest>$time.earliest$</earliest>
          <latest>$time.latest$</latest>
        </search>
        <option name="charting.chart">pie</option>
        <option name="charting.drilldown">none</option>
      </chart>
    </panel>
    <panel>
      <chart>
        <title>Top URIs</title>
        <search base="base_search">
          <query>| search httpRequest.clientIp="$client_ip$" httpRequest.clientIp="$blocked_ip$" httpRequest.country="$country$" | stats count by uri_query</query>
          <earliest>$time.earliest$</earliest>
          <latest>$time.latest$</latest>
        </search>
        <option name="charting.chart">pie</option>
        <option name="charting.drilldown">none</option>
      </chart>
    </panel>
  </row>
  <row>
    <panel>
      <table>
        <title>Statistics Table</title>
        <search base="base_search">
          <query>| search httpRequest.clientIp="$client_ip$" httpRequest.clientIp="$blocked_ip$" httpRequest.country="$country$"
          | table httpRequest.country,httpRequest.clientIp,url,httpRequest.httpMethod,time,date</query>
          <earliest>$time.earliest$</earliest>
          <latest>$time.latest$</latest>
        </search>
        <option name="drilldown">none</option>
      </table>
    </panel>
  </row>
</form>
